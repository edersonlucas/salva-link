{
  "name": "salvalink-backend",
  "version": "0.1.0",
  "description": "Application backend SalvaLink",
  "main": "src/server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node ./build/server.js",
    "dev": "ts-node-dev ./src/server.ts",
    "lint": "eslint ./src --ext .ts",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:create": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test": "mocha -r ts-node/register ./src/tests/*.{test,spec}.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test"
  },
  "keywords": [],
  "author": "Ederson Lucas",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/sequelize": "4.28.10",
    "@types/sinon": "^10.0.13",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sequelize-cli": "6.6.0",
    "sinon": "^15.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "2.3.3",
    "puppeteer": "^19.5.2",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.0"
  }
}
